{
    "name": "vs-utils",
    "displayName": "vs-utils",
    "description": "Personal utilities for VS Code",
    "publisher": "KyleMit",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.48.0"
    },
    "icon": "images/code-cog.png",
    "galleryBanner": {
        "color": "#8db3f9"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "tools"
    ],
    "activationEvents": [
        "onCommand:vs-utils.refreshCspell",
        "onCommand:vs-utils.removeDuplicates"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
                "command": "vs-utils.refreshCspell",
                "title": "Refresh Spell Checker",
                "category": "Utils"
            },
            {
                "command": "vs-utils.removeDuplicates",
                "title": "Remove Duplicate Lines",
                "category": "Utils",
                "enablement": "editorHasSelection"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "deploy": "vsce publish"
    },
    "devDependencies": {
        "@types/node": "^14.0.27",
        "@types/vscode": "^1.48.0",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "eslint": "^7.6.0",
        "typescript": "^3.8.3",
        "vsce": "^1.79.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/KyleMit/vs-utils.git"
    },
    "license": "MIT"
}