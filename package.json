{
    "name": "vs-utils",
    "displayName": "vs-utils",
    "description": "Personal utilities for VS Code",
    "publisher": "KyleMit",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.48.0"
    },
    "icon": "images/code-cog.png",
    "categories": ["Other"],
    "keywords": ["tools"],
    "activationEvents": [
        "onCommand:vs-utils.refreshCspell"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "vs-utils.refreshCspell",
            "title": "Utils: Refresh Spell Checker"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.48.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^14.0.27",
        "eslint": "^7.6.0",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "glob": "^7.1.6",
        "mocha": "^8.0.1",
        "typescript": "^3.8.3",
        "vscode-test": "^1.4.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/KyleMit/vs-utils.git"
    }
}